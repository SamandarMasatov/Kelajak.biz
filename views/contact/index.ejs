<div class="row">
    <div class="col-12">
      <div
        style="
          display: flex;
          flex-direction: row;
          justify-content: space-between;
          align-items: center;
        "
      >
        <h4 class="m-3" style="font-weight: 700;">Yangi so'rovnomalar</h4>
        
         
          <a class="btn btn-primary waves-effect waves-light" href="/contact/seen"> Ko'rib chiqilganlar ro'yhati</a>
      </div>
  
      <div class="card m-b-30">
        <div class="card-body">
         <div style="overflow-x: scroll;">
          <table id="datatable" class="table table-bordered text-center" style="border-collapse: collapse; border-spacing: 0; width: 100%" >
            <thead>
              <tr>
                <th>Rasm</th>
                <th>Foydalanuvchi</th>
                <th>Telefon raqam</th>
                <th>Pochta</th>
                <th>Yaratilgan sanasi</th>
                <th style="width: 10%">Sozlamalar</th>
              </tr>
            </thead>
  
            <tbody>
              <% for(let item of result) {%>
              <tr>
                <td>
                    <!-- <img src="/uploads/users" style="width: 50px;"> -->
                    <img src="/assets/images/users/user-4.jpg" style="width: 50px;" alt="user" >
                </td>
                <td><%= item.username.name %></td>
                <td><%= item.username.email %></td>
                <td><%= item.phone %></td>
                <td><%= moment(item.date).format("DD-MM-YYYY - hh:mm:ss") %></td>
                <td style=" display: flex; flex-direction: row; justify-content: space-around; align-items: center;" >
                  <a class="btn btn-success" href="<%=`/contact/info/${item._id}`%>">
                      <i class="fa fa-eye"></i>
                  </a>
                  <a class="btn btn-light bg-dark" href="<%=`/contact/make/${item._id}`%>">
                    <i class="far fa-eye-slash text-light"></i>
                </a>
                  
                </td>
              </tr>
              <% } %>
            </tbody>
          </table>
         </div>
        </div>
      </div>
    </div>
    <!-- end col -->
  </div>

