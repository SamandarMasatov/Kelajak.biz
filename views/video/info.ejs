<div class="col-md-12">
    <div class="card m-b-30">
      <div class="card-body">
        <h4 class="mt-2 mb-4">Ushbu video haqida malumot</h4>
        <ul class="nav nav-pills nav-justified" role="tablist">
          <li class="nav-item waves-effect waves-light">
            <a class="nav-link active" data-toggle="tab" href="#home-1" role="tab">
              <span class="d-none d-md-block font-14">Video darslik haqida malumot</span>
              <span class="d-block d-md-none"><i class="mdi mdi-home-variant h5"></i></span>
            </a>
          </li>
         
        </ul>
        <div class="tab-content">
          <div class="tab-pane active p-3" id="home-1" role="tabpanel">
            <table class="table table-bordered border-primary text-center">
              <thead>
                <tr>
                  <th scope="col" style="font-weight: 600; width: 35%;">Bo'limlar</th>
                  <th scope="col" style="font-weight: 600;">Ma'lumotlar</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td>Video darslik nomi</td>
                  <td><%= result.title  %> </td>
                </tr>
                <tr>
                  <td>Videoga tegishli bolgan kurs</td>
                  <td><%= result.course_ID.title  %> </td>
                </tr>
                <tr>
                  <td>Videoga tegishli bolgan mavzu</td>
                  <td><%= result.section_ID.name %> </td>
                </tr>
                <tr>
                  <td>Video davomiyligi vaqti</td>
                  <td><%= result.video_time  %> </td>
                </tr>
                <tr>
                  <td>Video teglari</td>
                  <td><%= result.tag  %> </td>
                </tr>
                <tr>
                  <td>Video raytingi</td>
                  <td><%= result.rating  %> </td>
                </tr>
                <tr>
                  <td>Nechi marta ko'rib chiqilgan</td>
                  <td><%= result.counter  %> </td>
                </tr>
                <tr>
                  <td>Nechi haqida</td>
                  <td><%= result.description  %> </td>
                </tr>
                 
              </tbody>
            </table>
          </div>
         
          
  
        </div>
      </div>
    </div>
  </div>
  